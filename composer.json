{
  "name": "3xw/cakephp-bake",
  "description": "3xw CakePHP template skeleton",
  "type": "cakephp-plugin",
  "require": {
    "php": ">=7.2",
    "cakephp/cakephp": "^4.0",
    "cakephp/plugin-installer": "^1.0",
    "cakephp/twig-view": "^1.0"
    "friendsofcake/search": "^6.0"
  },
  "require-dev": {
    "cakephp/cakephp-codesniffer": "^4.0",
    "phpunit/phpunit": "~8.5.0"
  },
  "autoload": {
    "psr-4": {
      "Trois\\Bake\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Trois\\Bake\\Test\\": "tests",
      "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
    }
  }
}
