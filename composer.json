{
  "name": "3xw/cakephp-bake",
  "description": "3xw CakePHP template skeleton",
  "type": "cakephp-plugin",
  "require": {
    "php": ">=7.2",
    "cakephp/cakephp": "4.x-dev",
    "cakephp/plugin-installer": "^1.0",
    "friendsofcake/search": "cake-4.x-dev",
    "wyrihaximus/twig-view": "dev-master"
  },
  "require-dev": {
    "cakephp/cakephp-codesniffer": "dev-next",
    "phpunit/phpunit": "^8.0"
  },
  "autoload": {
    "psr-4": {
      "Trois\\Bake\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Trois\\Bake\\Test\\": "tests",
      "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
    }
  }
}
